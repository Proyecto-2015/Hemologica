<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd"
	version="3.2">
	
	

	<enterprise-beans>
		
		<session>
			<ejb-name>DonationBean</ejb-name>
			<business-local>org.hemologica.salud.ejb.beans.DonationBeanLocal</business-local>
			<ejb-class>org.hemologica.salud.ejb.beans.impl.DonationBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
			<persistence-context-ref>
				<persistence-context-ref-name>persistence/Hemologica-Salud-PU/entity-manager</persistence-context-ref-name>
				<persistence-unit-name>Hemologica-Salud-PU</persistence-unit-name>
			</persistence-context-ref>
			
		</session>
		<session>
			<ejb-name>TransfusionBean</ejb-name>
			<business-local>org.hemologica.salud.ejb.beans.TransfusionBeanLocal</business-local>
			<ejb-class>org.hemologica.salud.ejb.beans.impl.TransfusionBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
			<persistence-context-ref>
				<persistence-context-ref-name>persistence/Hemologica-Salud-PU/entity-manager</persistence-context-ref-name>
				<persistence-unit-name>Hemologica-Salud-PU</persistence-unit-name>
			</persistence-context-ref>
		</session>
		<session>
			<ejb-name>StockBean</ejb-name>
			<business-local>org.hemologica.salud.ejb.beans.StockBeanLocal</business-local>
			<ejb-class>org.hemologica.salud.ejb.beans.impl.StockBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
			<persistence-context-ref>
				<persistence-context-ref-name>persistence/Hemologica-Salud-PU/entity-manager</persistence-context-ref-name>
				<persistence-unit-name>Hemologica-Salud-PU</persistence-unit-name>
			</persistence-context-ref>
		</session>
		<session>
			<ejb-name>HistoryBean</ejb-name>
			<business-local>org.hemologica.salud.ejb.beans.HistoryBeanLocal</business-local>
			<ejb-class>org.hemologica.salud.ejb.beans.impl.HistoryBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
			<persistence-context-ref>
				<persistence-context-ref-name>persistence/Hemologica-Salud-PU/entity-manager</persistence-context-ref-name>
				<persistence-unit-name>Hemologica-Salud-PU</persistence-unit-name>
			</persistence-context-ref>
		</session>
		<session>
			<ejb-name>StatisticsBean</ejb-name>
			<business-local>org.hemologica.salud.ejb.beans.StatisticsBeanLocal</business-local>
			<ejb-class>org.hemologica.salud.ejb.beans.impl.StatisticsBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
			<persistence-context-ref>
				<persistence-context-ref-name>persistence/Hemologica-Salud-PU/entity-manager</persistence-context-ref-name>
				<persistence-unit-name>Hemologica-Salud-PU</persistence-unit-name>
			</persistence-context-ref>
		</session>
	</enterprise-beans>



</ejb-jar>