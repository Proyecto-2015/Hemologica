<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://xmlns.jcp.org/xml/ns/persistence
        http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">

	<persistence-unit name="Hemologica-YodonoDS-PU" transaction-type="JTA">
		<jta-data-source>java:/jboss/datasources/Hemologica-DS</jta-data-source>
		<class>org.hemologica.model.Advertisment</class>
		<class>org.hemologica.model.AdvertismentsContent</class>
		<class>org.hemologica.model.Center</class>
		<class>org.hemologica.model.Concept</class>
		<class>org.hemologica.model.ConceptsDisplay</class>
		<class>org.hemologica.model.CountriesCode</class>
		<class>org.hemologica.model.DocumentsTypesCode</class>
		<class>org.hemologica.model.Identification</class>
		<class>org.hemologica.model.IdentificationsHistory</class>
		<class>org.hemologica.model.Institution</class>
		<class>org.hemologica.model.Movement</class>
		<class>org.hemologica.model.MovementsType</class>
		<class>org.hemologica.model.Notification</class>
		<class>org.hemologica.model.NotificationsPerson</class>
		<class>org.hemologica.model.Permission</class>
		<class>org.hemologica.model.Person</class>
		<class>org.hemologica.model.PersonsRecord</class>
		<class>org.hemologica.model.Role</class>
		<class>org.hemologica.model.RolePermission</class>
		<class>org.hemologica.model.Service</class>
		<class>org.hemologica.model.Unit</class>
		<class>org.hemologica.model.User</class>
		<class>org.hemologica.model.UsersRoleService</class>
		<properties>
			<property name="hibernate.hbm2ddl.auto" value="none" />
			<property name="hibernate.show_sql" value="false" />
		</properties>
	</persistence-unit>
</persistence>
