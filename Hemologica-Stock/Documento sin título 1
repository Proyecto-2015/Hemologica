    FOREIGN KEY (`users_user_id`)
    REFERENCES `hemologica_stock`.`users` (`user_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `hemologica_stock`.`unit_types`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `hemologica_stock`.`unit_types` (
  `unit_type_id` INT NOT NULL AUTO_INCREMENT,
  `unit_type_name` VARCHAR(45) NOT NULL,
  `unit_type_description` VARCHAR(45) NULL,
  `unit_type_code` VARCHAR(45) NULL,
  PRIMARY KEY (`unit_type_id`),
  UNIQUE INDEX `unit_type_name_UNIQUE` (`unit_type_name` ASC))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `hemologica_stock`.`units`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `hemologica_stock`.`units` (
  `unit_id` BIGINT NOT NULL AUTO_INCREMENT,
  `unit_service_code` VARCHAR(45) NOT NULL,
  `unit_service_donation_code` VARCHAR(45) NOT NULL,
  `unit_type_id` INT NOT NULL,
  PRIMARY KEY (`unit_id`, `unit_type_id`),
  UNIQUE INDEX `unit_service_code_UNIQUE` (`unit_service_code` ASC),
  INDEX `fk_unit_unit_type1_idx` (`unit_type_id` ASC),
  CONSTRAINT `fk_unit_unit_type1`
    FOREIGN KEY (`unit_type_id`)
    REFERENCES `hemologica_stock`.`unit_types` (`unit_type_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `hemologica_stock`.`input_types`
