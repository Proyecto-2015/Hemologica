  `output_type_description` VARCHAR(45) NULL,
  PRIMARY KEY (`output_type_id`),
  UNIQUE INDEX `output_type_name_UNIQUE` (`output_type_name` ASC))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `hemologica_stock`.`outputs`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `hemologica_stock`.`outputs` (
  `output_id` BIGINT NOT NULL AUTO_INCREMENT,
  `output_description` VARCHAR(45) NULL,
  `output_date` DATE NOT NULL,
  `output_time_reg` VARCHAR(45) NOT NULL,
  `output_type_output_type_id` INT NOT NULL,
  `input_input_id` INT NOT NULL,
  `input_input_type_input_type_id` INT NOT NULL,
  `input_services_service_id` INT NOT NULL,
  PRIMARY KEY (`output_id`, `output_type_output_type_id`),
  INDEX `fk_output_output_type1_idx` (`output_type_output_type_id` ASC),
  INDEX `fk_output_input1_idx` (`input_input_id` ASC, `input_input_type_input_type_id` ASC, `input_services_service_id` ASC),
  CONSTRAINT `fk_output_output_type1`
    FOREIGN KEY (`output_type_output_type_id`)
    REFERENCES `hemologica_stock`.`output_types` (`output_type_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_output_input1`
    FOREIGN KEY (`input_input_id` , `input_input_type_input_type_id` , `input_services_service_id`)
    REFERENCES `hemologica_stock`.`inputs` (`input_id` , `input_type_input_type_id` , `services_service_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


