  `services_service_id` INT NOT NULL,
  `roles_role_id` BIGINT NOT NULL,
  `users_user_id` INT NOT NULL,
  PRIMARY KEY (`user_role_service_id`, `services_service_id`, `roles_role_id`, `users_user_id`),
  INDEX `fk_user_role_service_services1_idx` (`services_service_id` ASC),
  INDEX `fk_user_role_service_roles1_idx` (`roles_role_id` ASC),
  INDEX `fk_user_role_service_users1_idx` (`users_user_id` ASC),
  CONSTRAINT `fk_user_role_service_services1`
    FOREIGN KEY (`services_service_id`)
    REFERENCES `hemologica_stock`.`services` (`service_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_user_role_service_roles1`
    FOREIGN KEY (`roles_role_id`)
