-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `hemologica_stock`.`users` (
  `user_id` INT NOT NULL AUTO_INCREMENT,
  `user_code` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`user_id`),
  UNIQUE INDEX `user_code_UNIQUE` (`user_code` ASC))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `hemologica_stock`.`roles`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `hemologica_stock`.`roles` (
  `role_id` BIGINT NOT NULL AUTO_INCREMENT,
  `role_code` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`role_id`),
  UNIQUE INDEX `role_code_UNIQUE` (`role_code` ASC))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `hemologica_stock`.`perms`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `hemologica_stock`.`perms` (
  `perm_id` INT NOT NULL AUTO_INCREMENT,
