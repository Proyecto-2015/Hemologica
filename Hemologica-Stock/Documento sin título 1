  PRIMARY KEY (`perm_id`),
  UNIQUE INDEX `perm_code_UNIQUE` (`perm_code` ASC))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `hemologica_stock`.`role_perm`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `hemologica_stock`.`role_perm` (
  `role_perm_id` INT NOT NULL AUTO_INCREMENT,
  `roles_role_id` BIGINT NOT NULL,
  `perms_perm_id` INT NOT NULL,
  PRIMARY KEY (`role_perm_id`, `roles_role_id`, `perms_perm_id`),
  INDEX `fk_roles_perms_roles_idx` (`roles_role_id` ASC),
  INDEX `fk_roles_perms_perms1_idx` (`perms_perm_id` ASC),
  CONSTRAINT `fk_roles_perms_roles`
    FOREIGN KEY (`roles_role_id`)
    REFERENCES `hemologica_stock`.`roles` (`role_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_roles_perms_perms1`
    FOREIGN KEY (`perms_perm_id`)
    REFERENCES `hemologica_stock`.`perms` (`perm_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
