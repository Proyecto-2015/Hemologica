CREATE TABLE IF NOT EXISTS `hemologica_stock`.`input_types` (
  `input_type_id` INT NOT NULL AUTO_INCREMENT,
  `input_type_name` VARCHAR(45) NOT NULL,
  `input_type_description` VARCHAR(45) NULL,
  PRIMARY KEY (`input_type_id`),
  UNIQUE INDEX `input_type_name_UNIQUE` (`input_type_name` ASC))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `hemologica_stock`.`inputs`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `hemologica_stock`.`inputs` (
  `input_id` INT NOT NULL AUTO_INCREMENT,
  `input_description` VARCHAR(45) NULL,
  `input_date` DATE NOT NULL,
  `input_time_reg` TIMESTAMP NOT NULL,
  `input_type_input_type_id` INT NOT NULL,
  `services_service_id` INT NOT NULL,
  `unit_unit_id` BIGINT NOT NULL,
  PRIMARY KEY (`input_id`, `input_type_input_type_id`, `services_service_id`, `unit_unit_id`),
  INDEX `fk_input_input_type1_idx` (`input_type_input_type_id` ASC),
  INDEX `fk_input_services1_idx` (`services_service_id` ASC),
  INDEX `fk_input_unit1_idx` (`unit_unit_id` ASC),
  CONSTRAINT `fk_input_input_type1`
    FOREIGN KEY (`input_type_input_type_id`)
    REFERENCES `hemologica_stock`.`input_types` (`input_type_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_input_services1`
    FOREIGN KEY (`services_service_id`)
    REFERENCES `hemologica_stock`.`services` (`service_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_input_unit1`
    FOREIGN KEY (`unit_unit_id`)
    REFERENCES `hemologica_stock`.`units` (`unit_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `hemologica_stock`.`output_types`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `hemologica_stock`.`output_types` (
